.page-title Stories


.page-wiki-search-results

  - @my_drafts.each do |draft|
    :ruby
      drafted_hash = draft.drafted_hash
      drafted_hash = Marshal.load(drafted_hash)
 

    .wiki-page
      - if draft.model_id.blank?
        .ct=link_to drafted_hash[:how_to_demo], "/products/#{drafted_hash[:product_id]}/stories/new?temp_id=#{draft.temp_id}"
        .ct=link_to drafted_hash[:tips], "/products/#{drafted_hash[:product_id]}/stories/new?temp_id=#{draft.temp_id}"
      - else
        .ct=link_to drafted_hash[:how_to_demo], "/products/#{drafted_hash[:product_id]}/stories/#{draft.model_id}/edit?temp_id=#{draft.temp_id}"
        .ct=link_to drafted_hash[:tips], "/products/#{drafted_hash[:product_id]}/stories/#{draft.model_id}/edit?temp_id=#{draft.temp_id}"

  .paginate=will_paginate @my_drafts